{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "project": {
        "name": "ng-upgrade"
    },
    "apps": [{
        "root": "src",
        "outDir": "dist",
        "assets": [
            "assets",
            "favicon.ico",
            "app/components",
            "app/factories",
            "bower_components"
        ],
        "index": "index.html",
        "main": "main.ts",
        "polyfills": "polyfills.ts",
        "test": "test.ts",
        "tsconfig": "tsconfig.app.json",
        "testTsconfig": "tsconfig.spec.json",
        "prefix": "app",
        "styles": [
            "styles.css",
            "bower_components/bootstrap/dist/css/bootstrap.min.css",
            "bower_components/font-awesome/css/font-awesome.min.css",
            "app/components/list/list.css",
            "app/components/header/header.css"
        ],
        "scripts": [
            "bower_components/sprintf/dist/sprintf.min.js",
            "../node_modules/angular/angular.min.js",
            "../node_modules/angular-route/angular-route.min.js",
            "app/app.js",
            "app/factories/lookups.factory.js",
            "app/components/container/container.component.js",
            "app/components/header/header.component.js",
            "app/components/cards/cards.component.js",
            "app/components/cards/cards.service.js",
            "app/components/list/list.component.js",
            "app/components/person/person.component.js",
            "app/components/planet/planet.component.js",
            "app/components/film/film.component.js",
            "app/components/starship/starship.component.js",
            "app/components/vehicle/vehicle.component.js",
            "app/components/species/species.component.js",
            "app/components/upgrade-me/upgrade-me.component.js"
        ],
        "environmentSource": "environments/environment.ts",
        "environments": {
            "dev": "environments/environment.ts",
            "prod": "environments/environment.prod.ts"
        }
    }],
    "e2e": {
        "protractor": {
            "config": "./protractor.conf.js"
        }
    },
    "lint": [{
        "project": "src/tsconfig.app.json"
    }, {
        "project": "src/tsconfig.spec.json"
    }, {
        "project": "e2e/tsconfig.e2e.json"
    }],
    "test": {
        "karma": {
            "config": "./karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "css",
        "component": {}
    }
}
